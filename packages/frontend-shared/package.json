{
	"name": "frontend-shared",
	"type": "module",
	"main": "",
	"exports": {
		"./js/*": {
			"import": "./js-built/*.mjs",
			"types": "./js-built/*.d.ts"
		},
		"./themes/*": {
			"import": "./themes/*",
			"types": "./js-built/themes.d.ts"
		}
	},
	"scripts": {
		"build": "unbuild",
		"watch": "node ./watch.js",
		"eslint": "eslint './**/*.{js,jsx,ts,tsx}'",
		"typecheck": "tsc --noEmit",
		"lint": "pnpm typecheck && pnpm eslint"
	},
	"devDependencies": {
		"@types/node": "22.9.0",
		"@types/tinycolor2": "1.4.6",
		"@typescript-eslint/eslint-plugin": "7.17.0",
		"@typescript-eslint/parser": "7.17.0",
		"eslint-plugin-vue": "9.31.0",
		"tsc-alias": "1.8.10",
		"typescript": "5.6.3",
		"unbuild": "^3.2.0",
		"vue-eslint-parser": "9.4.3"
	},
	"files": [
		"js-built",
		"themes"
	],
	"dependencies": {
		"misskey-js": "workspace:*",
		"shiki": "1.22.2",
		"tinycolor2": "1.6.0",
		"vue": "3.5.12"
	}
}
